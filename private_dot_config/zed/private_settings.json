{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "theme": {
    "light": "Github Light",
    "dark": "Everforest Dark"
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_line_height": "standard",
  "soft_wrap": "editor_width",
  "buffer_font_size": 16,
  "autosave": "on_focus_change",
  "cursor_blink": false,
  "journal": {
    "path": "~/notes",
    "hour_format": "hour24"
  },
  "auto_update": false
}
